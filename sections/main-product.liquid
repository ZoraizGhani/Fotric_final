                        <g clip-path="url(#clip0_1047_3889)">
                        <path fill-rule="evenodd" clip-rule="evenodd" d="M15.9945 14.3883L7.98038 6.37426L9.98355 4.37109L16.9961 11.3836L24.0086 4.37109L26.0117 6.37426L17.9976 14.3883C17.732 14.6539 17.3717 14.8031 16.9961 14.8031C16.6204 14.8031 16.2601 14.6539 15.9945 14.3883Z" fill="black"/>
                        </g>
                        <defs>
                        <clipPath id="clip0_1047_3889">
                        <rect width="17" height="34" fill="white" transform="matrix(0 1 -1 0 34 0)"/>
