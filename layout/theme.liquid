<script>
document.addEventListener('DOMContentLoaded', function () {
  const productForm = document.querySelector('product-form form');
  if (!productForm) return;

  let isSubmitting = false;

  productForm.addEventListener('submit', function (e) {
    e.preventDefault();
    if (isSubmitting) return;
    isSubmitting = true;

    const items = [];

    // Bundle products only
    const bundleCards = productForm.querySelectorAll('.bundle-card');

    bundleCards.forEach(card => {
      const checkbox = card.querySelector('.bundle-checkbox');
      const qtyInput = card.querySelector('.bundle-qty-input');

      if (checkbox?.checked && !checkbox.disabled) {
        items.push({
          id: checkbox.dataset.variantId,
          quantity: parseInt(qtyInput?.value, 10) || 1
        });
      }
    });

    // If no bundle selected, stop
    if (!items.length) {
      isSubmitting = false;
      return;
    }

    fetch('/cart/add.js', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json',
        'Accept': 'application/json'
      },
      body: JSON.stringify({ items })
    })
    .then(res => {
      if (!res.ok) throw new Error('Add to cart failed');

      // Reset bundle quantities & checkboxes
      productForm.querySelectorAll('.bundle-qty-input').forEach(i => i.value = 1);
      productForm.querySelectorAll('.bundle-checkbox').forEach(c => c.checked = false);

      window.location.href = '/cart';
    })
    .catch(err => {
      console.error('Bundle add error:', err);
      isSubmitting = false;
    });
  });
});
</script>

